{
  "name": "sdb-admin",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "antd": "^4.19.2",
    "antd-img-crop": "^4.2.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.3",
    "react-dom": "^17.0.2",
    "react-multiple-select-dropdown-lite": "^2.0.4",
    "react-quill": "^1.3.5",
    "react-quill-2.0": "^1.0.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.1",
    "react-scripts": "4.0.3",
    "react-select": "^5.2.1",
    "recharts": "^2.1.8",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "indent": [
        "error",
        "tab",
        {
          "SwitchCase": 1,
          "ignoredNodes": [
            "ConditionalExpression",
            "TemplateLiteral"
          ]
        }
      ],
      "no-tabs": 0,
      "semi": [
        1,
        "never"
      ],
      "react/display-name": 0,
      "template-curly-spacing": "off",
      "react/prop-types": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.5.0"
  }
}
