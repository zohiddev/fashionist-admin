{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["react-app", "react-app/jest", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": [1, "never"],
        "react/display-name": 0,
        "react/prop-types": 0
    }
}
